<%- include('partials/header') %>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fas fa-key fa-6x"></i>
    <h1 class="display-3">You've Discovered My Secret!</h1>


    
    <hr>
    
    <a class="btn btn-outline-danger btn-lg" href="/logout" role="button">Log Out</a>
    <a class="btn btn-outline-info btn-lg" href="/submit" role="button">Submit a Secret</a>
  </div>
</div>

<div class="container">
  
  <% secrets.forEach(function(user){ %>
      <p class="secret-text"><%=user.secret%> </p>    
  <% }); %>
</div>



<%- include('partials/footer') %>
